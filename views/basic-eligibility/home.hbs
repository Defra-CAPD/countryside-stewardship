<a class="back-link" href="/">Back to start</a>


<form method="post" action="/submission/">


  <h1 class="form-title heading-xlarge">Basic eligibility</h1>


  <div class="form-group form-group-compound">
  
    <fieldset class="inline">

      <legend class="form-label">Do you have 5 year management control of the land?</legend>
  
      <div class="form-group">
  
        <label class="block-label" for="radio-inline-1">
          <input id="radio-inline-1" name="management" value="Yes" type="radio"> Yes
        </label>
  
        <label class="block-label" for="radio-inline-2">
          <input id="radio-inline-2" name="management" value="No" type="radio"> No
        </label>
  
      </div>
  
    </fieldset>
    
  </div>
  
  
  <div class="form-group form-group-compound">
    
    <fieldset class="inline">

      <legend class="form-label">Do you have an existing agreement?</legend>
  
      <div class="form-group">
  
        <label class="block-label" for="radio-inline-3">
          <input id="radio-inline-3" name="agreement" value="Yes" type="radio"> Yes
        </label>
  
        <label class="block-label" for="radio-inline-4">
          <input id="radio-inline-4" name="agreement" value="No" type="radio"> No
        </label>
  
      </div>
  
    </fieldset>
    
  </div>
  
  
  <div class="form-group">
    
    <input class="button" type="submit" value="Continue">

  </div>
  

</form>